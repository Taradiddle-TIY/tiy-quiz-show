

<div id="welcome_page">

  <header>
       <div class="container header-class" id="maincontent" tabindex="-1">
           <div class="row">
               <div class="col-lg-12">
                   <img class="img-responsive" src="" alt="">
                   <div class="intro-text">
                       <h1 class="name">Quiz Show</h1>
                       <hr class="star-light">
                       <span id="join" class="skills">check your brain</span>

                   </div>
               </div>
           </div>
       </div>
   </header>

   <div class="container join-us">
      <h3 id='join' class="lead"> join us? </h3>
        <div class="text-center" >
          <button id="button-text" class="btn btn-default" type="submit"><%= link_to "Sign up", new_user_path %></button>
        </div>
  </div>

    <h2 class="top-scores">TOP SCORES!!!</h2>

    <table class="table table-striped">
      <tr>
        <th>User</th>
        <th>Percent Correct</th>
      </tr>
      <% @complete_submissions.each do |submission| %>
      <tr>
        <td><%= User.find(submission.user_id).name %></td>
        <td><%= submission.correct / submission.possible %></td>
      </tr>
      <% end %>
    </table>


</div>
